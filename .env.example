# OpenAI API Configuration
# Get your API key from https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Alternative: OpenRouter API (for accessing multiple models)
# Get your API key from https://openrouter.ai/keys
OPENROUTER_API_KEY=your_openrouter_api_key_here

# OnchainKit Configuration
# Get your API key from https://portal.cdp.coinbase.com/products/onchainkit
NEXT_PUBLIC_ONCHAINKIT_API_KEY=your_onchainkit_api_key_here

# Next.js Configuration
NEXT_PUBLIC_APP_NAME=DreamWeaver
NEXT_PUBLIC_APP_DESCRIPTION=AI-powered dream interpretation and pattern analysis

# Wallet Configuration (for OnchainKit)
NEXT_PUBLIC_WALLET_CONNECT_PROJECT_ID=your_wallet_connect_project_id
NEXT_PUBLIC_CHAIN_ID=8453
